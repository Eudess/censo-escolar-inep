    <connection>BaseCenso</connection>
    <sql>SELECT tblTemp1.COD_ESCOLA, ID_NE,ID_RACA, ID_SEXO, REGIAO, ESTADO, QTD_ALUNOS_DEF, QTD_ALUNOS_TOTAL
FROM (SELECT COALESCE(E.COD_ESCOLA,0) AS COD_ESCOLA,
				COALESCE(NE.ID,0) AS ID_NE,
				COALESCE(R.ID,0) AS ID_RACA,
				COALESCE(S.ID,0) AS ID_SEXO,
				COALESCE(L.COD_REGIAO, '0') AS REGIAO,
				COALESCE(L.COD_UF, '0') AS ESTADO, 
                count(*) as QTD_ALUNOS_DEF

FROM ESCOLA E, LOCALIZACAO L,
			NECESSIDADE_ESPECIAL NE 
            INNER JOIN ALUNO_HAS_NECESSIDADE_ESPECIAL HNE
				ON NE.ID = HNE.NECESSIDADE_ESPECIAL_ID
			RIGHT JOIN ALUNO A 
				ON HNE.ALUNO_ID_PESSOA_FISICA = A.ID_PESSOA_FISICA, 
			RACA R, SEXO S                                   

WHERE A.ESCOLA_COD_ESCOLA = E.COD_ESCOLA AND A.RACA_ID = R.ID AND A.SEXO_ID = S.ID AND E.LOCALIZACAO_ID = L.ID 
GROUP BY COD_ESCOLA,NE.ID,R.ID,S.ID,L.COD_REGIAO,L.COD_UF) as tblTemp1
LEFT JOIN 
(SELECT COALESCE(E.COD_ESCOLA,0) AS COD_ESCOLA, 
                count(*) as QTD_ALUNOS_TOTAL
FROM ESCOLA E, LOCALIZACAO L,
			NECESSIDADE_ESPECIAL NE 
            INNER JOIN ALUNO_HAS_NECESSIDADE_ESPECIAL HNE
				ON NE.ID = HNE.NECESSIDADE_ESPECIAL_ID
			RIGHT JOIN ALUNO A 
				ON HNE.ALUNO_ID_PESSOA_FISICA = A.ID_PESSOA_FISICA, 
			RACA R, SEXO S                                   

WHERE A.ESCOLA_COD_ESCOLA = E.COD_ESCOLA AND A.RACA_ID = R.ID AND A.SEXO_ID = S.ID AND E.LOCALIZACAO_ID = L.ID
GROUP BY COD_ESCOLA) as tblTemp2 
ON tblTemp1.COD_ESCOLA = tblTemp2.COD_ESCOLA;</sql>
